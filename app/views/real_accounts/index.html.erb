<%- model_class = RealAccount -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:startdate) %></th>
      <th><%= model_class.human_attribute_name(:ip) %></th>
      <th><%= model_class.human_attribute_name(:firstname) %></th>
      <th><%= model_class.human_attribute_name(:lastname) %></th>
      <th><%= model_class.human_attribute_name(:fb_email) %></th>
      <th><%= model_class.human_attribute_name(:fb_pw) %></th>
      <th><%= model_class.human_attribute_name(:fb_profile_link) %></th>
      <th><%= model_class.human_attribute_name(:paypal_email) %></th>
      <th><%= model_class.human_attribute_name(:referrer_paypal) %></th>
      <th><%= model_class.human_attribute_name(:phone) %></th>
      <th><%= model_class.human_attribute_name(:disbursement_date) %></th>
      <th><%= model_class.human_attribute_name(:disbursement_amount) %></th>
      <th><%= model_class.human_attribute_name(:skype_username) %></th>
      <th><%= model_class.human_attribute_name(:notes) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @real_accounts.each do |real_account| %>
      <tr>
        <td><%= link_to real_account.id, real_account_path(real_account) %></td>
        <td><%= real_account.startdate %></td>
        <td><%= real_account.ip %></td>
        <td><%= real_account.firstname %></td>
        <td><%= real_account.lastname %></td>
        <td><%= real_account.fb_email %></td>
        <td><%= real_account.fb_pw %></td>
        <td><%= real_account.fb_profile_link %></td>
        <td><%= real_account.paypal_email %></td>
        <td><%= real_account.referrer_paypal %></td>
        <td><%= real_account.phone %></td>
        <td><%= real_account.disbursement_date %></td>
        <td><%= real_account.disbursement_amount %></td>
        <td><%= real_account.skype_username %></td>
        <td><%= real_account.notes %></td>
        <td><%=l real_account.created_at %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      real_account_path(real_account), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_real_account_path(real_account), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      real_account_path(real_account),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_real_account_path,
            :class => 'btn btn-primary' %>
